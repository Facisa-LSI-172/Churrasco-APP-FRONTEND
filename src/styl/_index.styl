.date
  // background: #ccc;

  h1
    font-size: 25px;
    line-height: 110%;

  small
    font-size: 9px;
    line-height: 115%;  
    display: block;


.container-contadores
  width: 100%
  
  .item-contador
    width: 50%
    display: inline
    float left
    margin-bottom: 40px

    .number-cont
      width 70px
      height @width
      display: block
      position: relative
      background: #ccc
      border-radius(50%)
      margin 0 auto

    .number
      font-size 40px
      color: #666
      text-align: center
      position: absolute
      top 50%
      left 50%
      transform(translate(-50%, -50%))
    
    span
      width: 100%
      display: table
      font-size: 12px
      color: #666
      text-align: center
      margin: 5px auto 0 auto
      